-- helpers

update_counter=0
t=0

function draw_as_sprite(value, x, y)
	startx=x;
	numstr=tostr(value)
	for i=1, #numstr do
		thischar=ord(numstr, i)
		spr(thischar+16, startx, y)
		startx+=8
	end

end

function update_timer()
	t+=1
	update_counter+=1
	if(update_counter>30) then
		level_timer-=1
		update_counter=0
	end
end

function hcenter(t)
	return 64-#t*2
end

function get_anim_frame(frames, do_anim)
	if(do_anim==true) then
		return frames[flr(t/8)%#frames+1]
	end
	
	return frames[1]
end
