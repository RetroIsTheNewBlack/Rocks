-- enemie stuff

enemies={}

type_bug=18
type_xx=80
type_yy=81
type_ufo=82

e_dir_left=20
e_dir_down=19
e_dir_right=18
e_dir_up=21

enemy_update_counter=0
enemy_anim=0

function create_enemies()
	for y=0,63 do
		for x=0,127 do
			if(mget(x,y)==type_bug) then
				this_enemy={}
				this_enemy.x=x
				this_enemy.y=y
				this_enemy.type=type_bug
				this_enemy.dir=e_dir_left
				
				add(enemies, this_enemy)	
				elseif(mget(x,y)==type_xx) then
					this_enemy={}
					this_enemy.x=x
					this_enemy.y=y
					this_enemy.type=type_xx
					this_enemy.dir=e_dir_right

					add(enemies,this_enemy)
				
				elseif(mget(x,y)==type_yy) then
					this_enemy={}
					this_enemy.x=x
					this_enemy.y=y
					this_enemy.type=type_yy
					this_enemy.dir=e_dir_up

					add(enemies,this_enemy)

				elseif(mget(x,y)==type_ufo) then
					this_enemy={}
					this_enemy.x=x
					this_enemy.y=y
					this_enemy.type=type_ufo
					this_enemy.dir=e_dir_left

					add(enemies,this_enemy)							
			end
		end
	end

end

function update_enemies()

	next_anim()

	if(enemy_update_timer()==true) then
		for e in all(enemies) do
			-- move a bug
			if(e.type==type_bug) then
				if(e.dir==e_dir_right) then
					if(mget(e.x+1,e.y)==empty) then
						mset(e.x, e.y, empty)
						mset(e.x+1, e.y, e_dir_right+enemy_anim)
						e.x+=1
					else
						e.dir=e_dir_down
					end	
		
				elseif(e.dir==e_dir_down) then
					if(mget(e.x,e.y+1)==empty) then
						mset(e.x, e.y, empty)
						mset(e.x, e.y+1, e_dir_down+enemy_anim)
						e.y+=1
					else
						e.dir=e_dir_left
					end	

				elseif(e.dir==e_dir_left) then
					if(mget(e.x-1,e.y)==empty) then
						mset(e.x, e.y, empty)
						mset(e.x-1, e.y, e_dir_left+enemy_anim)
						e.x-=1
					else
						e.dir=e_dir_up
					end	

				elseif(e.dir==e_dir_up) then
					if(mget(e.x,e.y-1)==empty) then
						mset(e.x, e.y, empty)
						mset(e.x, e.y-1, e_dir_up+enemy_anim)
						e.y-=1
					else
						e.dir=e_dir_right
					end	
				end
				
			elseif(e.type==type_xx) then
				sfx(8)
				if(e.dir==e_dir_right) then
					if(mget(e.x+1,e.y)==empty) then
						mset(e.x,e.y,empty)
						mset(e.x+1,e.y,e.type+enemy_anim)
						e.x+=1
					else
						e.dir=e_dir_left
					end
				end

				if(e.dir==e_dir_left) then
					if(mget(e.x-1,e.y)==empty) then
						mset(e.x,e.y,empty)
						mset(e.x-1,e.y,e.type+enemy_anim)
						e.x-=1
					else
						e.dir=e_dir_right
					end
				end
			
			elseif(e.type==type_yy) then
				sfx(10)
				if(e.dir==e_dir_up) then
					if(mget(e.x,e.y-1)==empty) then
						mset(e.x,e.y,empty)
						mset(e.x,e.y-1,e.type+enemy_anim)
						e.y-=1
					else
						e.dir=e_dir_down
					end
				end

				if(e.dir==e_dir_down) then
					if(mget(e.x,e.y+1)==empty) then
						mset(e.x,e.y,empty)
						mset(e.x,e.y+1,e.type+enemy_anim)
						e.y+=1
					else
						e.dir=e_dir_up
					end
				end

			elseif(e.type==type_ufo) then
				sfx(9)
				
				enx=e.x
				eny=e.y
				
				if(player1.x<e.x and mget(e.x-1,e.y)==empty) then
					enx=e.x-1
				
				elseif(player1.x>e.x and mget(e.x+1,e.y)==empty) then
					enx=e.x+1			
				
				elseif(player1.y<e.y and mget(e.x,e.y-1)==empty) then
					eny=e.y-1
				
				elseif(player1.y>e.y and mget(e.x,e.y+1)==empty) then
					eny=e.y+1
				end
				
				if(enx!=e.x or eny!=e.y) then
						mset(e.x,e.y,empty)
						mset(enx,eny,e.type+enemy_anim)
						e.x=enx
						e.y=eny
				end
			
			end
		end
	
	end

end


function enemy_update_timer()
	enemy_update_counter+=1
	
	if(enemy_update_counter>10) then
		enemy_update_counter=0
		return true
	end

	return false
	
end

function is_enemy_at(x,y)
	for e in all(enemies) do
		if(e.x==x and e.y==y) then
			return true
  end	
	end
	
	return false
end

function player_interact()
	if(is_enemy_at(player1.x, player1.y)) then
		sfx(6)
		game_active=false
	end
end

function next_anim()
	if(enemy_anim==0) then 
		enemy_anim=4
	else
		enemy_anim=0
	end
end
