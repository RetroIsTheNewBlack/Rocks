__lua__
level=1
level_num_diamonds=0
level_min_diamonds=20
level_timer=180
player1_score=0

game_active=false

game_won=false

debug_text=""

-- startscreen stuff
show_startscreen=true
bg_counter=0
bg_y=8

function _init()
	init_map()
	init_player_1()
	init_rain()
end


function _update()

	if(level_num_diamonds>=level_min_diamonds) then
		open_exit()
	end

 update_timer()
 
 if(level_timer<=0) game_active=false;

	if(game_active==true) then
		update_player_1()
		catch_up()
	 update_enemies()
		update_map()
		player_interact()
	elseif(show_startscreen==true) then
		check_sc_controls()
	else
		let_it_rain()
		check_sc_controls()
		-- todo what happens if we
		-- loose?
	end
end


function _draw()

	cls()
	
	if(game_active==true) then
	
		draw_map()
		rectfill(0+camx, 0+camy, 127+camx, 7+camy, 0)
		draw_as_sprite(level_num_diamonds, 0+camx, 0+camy)
		draw_as_sprite(level_min_diamonds, 32+camx, 0+camy)
		draw_as_sprite(level_timer, 64+camx, 0+camy)
		draw_as_sprite(player1_score, 104+camx, 0+camy)
	
		draw_player_1()
		
		--print(debug_text,0+camx, 0+camy, 7)
	elseif(game_active==false and show_startscreen==true) then
		draw_startscreen()
	elseif(game_active==false and game_won==false) then
	 camera(0,0)
		print("game over",45,50,7)
	print("press ❎ for 1 player game",13,100)
		
	else
		camera(0,0)
		draw_rain()
		print("♥♥♥ you win! ♥♥♥",20,50,7)
		print("your score:", hcenter("your score:"), 60, 7) 
	 print(player1_score, hcenter(tostr(player1_score)), 70, 7)
	end


end

function new_1_player_game()
	show_startscreen=false
	game_active=true
	game_won=false
	level=1
	player1_score=0
	
	level_num_diamonds=0
	level_min_diamonds=20
	level_timer=180
	
	init_player_1()
	
end

function check_sc_controls()
	if(btnp(❎)) then
		new_1_player_game()
	end
end

function draw_startscreen()

	bg_counter+=1
	
	if(bg_counter>2) then
		bg_counter=0
		bg_y-=1
		if(bg_y<=0) bg_y=8
	end

	for y=0,16 do
		for x=0,16 do
			spr(54,x*8, y*8-bg_y)
		end
	end

	spr(128,40,50,7,2)
	print("by retro is the new black", 14,70)	
	print("press ❎ for 1 player game",13,100)
end
