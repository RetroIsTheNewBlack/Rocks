-- player stuff

function init_player_1()
	player1={}
	player1.x=1
	player1.y=1
	player1.off_x=0
	player1.off_y=0
	player1.score=0
	player1.anim_frames={1,2,3}
	player1.anim=false
	
	player1.speed=8
	player1.dir_left=false
end

function update_player_1()
	newx=player1.x
	newy=player1.y
	
	b_left=false
	b_right=false
	b_up=false
	b_down=false

	if(btnp(⬅️)) then
		if(btn(❎)) then
			if(mget(player1.x-1, player1.y)==soil) then
				mset(player1.x-1, player1.y, empty)
			elseif(fget(mget(player1.x-1, player1.y),diamond)) then
				sfx(1)
				level_num_diamonds+=1
				player1_score+=10
				mset(player1.x-1, player1.y, empty)			
			end
		else
			newx-=1;
			player1.dir_left=true;
			b_left=true
		end
	end

	if(btnp(➡️)) then
		if(btn(❎)) then
			if(mget(player1.x+1, player1.y)==soil) then
				mset(player1.x+1, player1.y, empty)
			elseif(fget(mget(player1.x+1, player1.y),diamond)) then
				sfx(1)
				level_num_diamonds+=1
				player1_score+=10
				mset(player1.x+1, player1.y, empty)			
			end
		else
		 newx+=1;
		 player1.dir_left=false;
		 b_right=true
		end
	end
	
	if(btnp(⬆️)) then
		if(btn(❎)) then
			if(mget(player1.x, player1.y-1)==soil) then
				mset(player1.x, player1.y-1, empty)
			elseif(fget(mget(player1.x, player1.y-1),diamond)) then
				sfx(1)
				level_num_diamonds+=1
				player1_score+=10
				mset(player1.x, player1.y-1, empty)			
			end
		else
			newy-=1;
			b_up=true
		end
	end

	if(btnp(⬇️)) then
		if(btn(❎)) then
			if(mget(player1.x, player1.y+1)==soil) then
				mset(player1.x, player1.y+1, empty)
			elseif(fget(mget(player1.x, player1.y+1),diamond)) then
				sfx(1)
				level_num_diamonds+=1
				player1_score+=10
				mset(player1.x, player1.y+1, empty)			
			end
		else
			newy+=1
			b_down=true
		end
	end	
	
	if(can_move_to(newx, newy, b_left, b_right)) then
		tile=mget(newx, newy)
		
		player1.anim=true
		
		if(b_left)	player1.off_x=8
		if(b_up)	player1.off_y=8
		if(b_right)	player1.off_x=-8
		if(b_down)	player1.off_y=-8
		
		if(fget(tile, diamond)) then
			level_num_diamonds+=1
			mset(newx, newy, empty)
			set_gravity(newx, newy-1, false)
			sfx(1)
			player1_score+=10
		elseif(tile==soil) then
			mset(newx, newy, empty)
			sfx(0)
		elseif(tile==rock) then
			if(b_left==true and mget(newx-1, newy)==empty) then
				mset(newx, newy, empty)
				mset(newx-1, newy, rock)
			end
	
			if(b_right==true and mget(newx+1, newy)==empty) then
				mset(newx, newy, empty)
				mset(newx+1, newy, rock)
			end
		end


		player1.x=mid(0,newx,127);
		player1.y=mid(0,newy,63);;
	end
	
	
end


function draw_player_1()
 spr(get_anim_frame(player1.anim_frames, player1.anim),player1.x*8+player1.off_x, player1.y*8+map_offset_y+player1.off_y, 1, 1, player1.dir_left)
end

function is_player_at(x,y)
	if(player1.x==x and player1.y==y) return true
	
	return false	
end

function catch_up()
	has_moved=false
	
	if(player1.off_x>0) then
		player1.off_x-=2
		has_moved=true
	elseif(player1.off_x<0) then
	 player1.off_x+=2
	 has_moved=true
	end
	
	if(player1.off_y>0) then
		player1.off_y-=2
		has_moved=true
	elseif(player1.off_y<0) then
	 player1.off_y+=2
	 has_moved=true
	end

	if(has_moved==false) then
		player1.anim=false
	end

end
