-- diamond rain

rain={}

function init_rain()
	for i=0,15 do
		dia={}
		dia.x=i
		dia.y=0
		dia.speed=rnd(2)
		dia.type=26+rnd(3)
		add(rain, dia)
	end
end

function let_it_rain()
	for d in all(rain) do
		d.y+=d.speed
		d.speed=d.speed*1.01
		
		if(d.y>127) then
			d.y=-8
			d.type=26+rnd(3)
			d.speed=rnd(2)

		end
		
	end
end

function draw_rain()
	for y=0, 13 do
		for x=0, 15 do
			spr(49,x*8, y*8)
		end
	end

	for d in all(rain) do
		spr(d.type,d.x*8, d.y)
	end

	for x=0, 15 do
		for d in all(rain) do
			if(d.x==x) then
				if(d.y>=112) then
					spr(15,x*8, 112)
					spr(31,x*8, 104)
		 	else
					spr(10,x*8, 112)
			 end
			end
 	end
	end

	for x=0, 15 do
		spr(10,x*8, 120)
	end

end
